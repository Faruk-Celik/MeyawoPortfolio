@model List<MeyawoPortfolio.Models.TblAbout>


@foreach (var item in Model)
{
    <section class="section pt-0" id="about">
        <!-- container -->
        <div class="container text-center">
            <!-- about wrapper -->
            <div class="about">
                <div class="about-img-holder">
                    <img src="@item.ImageUrl" class="about-img"
                         alt="-">
                </div>
                <div class="about-caption">
                    <p class="section-subtitle">@item.Title</p>
                    <h2 class="section-title mb-3">@item.Header</h2>
                    <p>
                        @item.Description
                    </p>
                  <a class="btn-rounded btn btn-outline-primary mt-4" href="/Templates/meyawo-1.0.0/meyawo-1.0.0/assets/MyCv/FarukCV.pdf" download="/Templates/meyawo-1.0.0/meyawo-1.0.0/assets/MyCv/FarukCV.pdf" role="button" > Download CV</a>
          
                </div>
            </div><!-- end of about wrapper -->
        </div><!-- end of container -->
    </section>
    }
   
