@model MeyawoPortfolio.Models.TblFuture
@{
    ViewBag.Title = "UpdateFuture";
    Layout = "~/Views/AdminLayout/_Layout.cshtml";
}

<h2> Edit Feature </h2>
<form method="post">
    <label> # </label>
    <input type="text" class="table table-bordered" value="@Model.FutureID" name="FutureID" />
    <br />
    <label> Header </label>
    <input type="text" class="table table-bordered" value="@Model.Header" name="Header" />
    <br />
    <label> Name Surname </label>
    <input type="text" class="table table-bordered" value="@Model.NameSurname" name="NameSurname" />
    <br />
    <label> Title </label>
    <input type="text" class="table table-bordered" value="@Model.Title" name="Title" />
    <br />
    <button class="btn btn-info"> Submit </button>
    <a href="/Future/Index/" class="btn btn-outline-warning"> Go Back </a>


</form>

