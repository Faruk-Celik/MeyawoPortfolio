@model List <MeyawoPortfolio.Models.TblProject>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/_Layout.cshtml";
}
<h2> project List</h2>

<table class="table table-bordered">
   <tr>
       <th> Project Id</th>
       <th> Project Title</th>
       <th> Project Categories</th>
       <th> Delete</th>
       <th> Update</th>
       <th> Github Links</th>
   </tr>
    @foreach (var item in Model)
            {
        <tr>
            <td>@item.ProjectID</td>
            <td>@item.Title</td>
            <td>@item.ProjectCategory</td>
            <td><a href="#" class="btn btn-danger">Delete</a></td>
            <td><a href="#" class="btn btn-success">Update</a></td>
            <td><a href="#" class="btn btn-dark">Open with Github</a></td>
        </tr>
            }
</table>
<a href="#" class="btn btn-primary">Add New Project</a>
